{
  "name": "@ohtomi/react-handsontable-hoc",
  "version": "0.5.1",
  "description": "A higher order component for react-handsontable",
  "keywords": [
    "react",
    "handsontable",
    "sort-columns",
    "move-columns",
    "hide-columns",
    "filter-rows"
  ],
  "homepage": "https://github.com/ohtomi/react-handsontable-hoc#readme",
  "bugs": {
    "url": "https://github.com/ohtomi/react-handsontable-hoc/issues"
  },
  "license": "MIT",
  "author": "Kenichi Ohtomi",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohtomi/react-handsontable-hoc.git"
  },
  "scripts": {
    "build": "run-s lint flow compile test",
    "clear": "rimraf dist coverage .nyc_output",
    "flow": "flow",
    "lint": "eslint src/ test/",
    "compile": "cross-env webpack --progress --profile",
    "test": "cross-env NODE_ENV=test ava --verbose",
    "cover": "nyc --reporter=html --reporter=text npm run test",
    "start": "run-s storybook",
    "storybook": "start-storybook -p 9080",
    "watch": "run-p watch:*",
    "watch:src": "chokidar src/**/*.js -c \"npm run compile\"",
    "watch:test": "chokidar test/**/*.js -c \"cross-env NODE_ENV=test ava --tap {path} | tap-notify\""
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/react": "^3.3.9",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "ava": "^1.0.0-beta.7",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.1.3",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.1.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-ava": "^5.1.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-security": "^1.4.0",
    "flow-bin": "^0.80.0",
    "flow-typed": "^2.2.3",
    "handsontable": "^5.0.0",
    "jsdom": "^12.0.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^12.0.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-handsontable": "^0.3.1",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.3",
    "tap-notify": "^1.0.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  },
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "sources": [
      "src/**/*.{js,jsx}"
    ],
    "require": [
      "./test/setup.js"
    ],
    "babel": {
      "testOptions": {}
    }
  }
}
